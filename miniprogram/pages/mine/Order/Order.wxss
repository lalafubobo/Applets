/* pages/mine/Order/Order.wxss */

.page{
  width: 100%;
  height: 800rpx;
  font-family: '微软雅黑';
  padding: 0rpx;
  margin: 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.page-middle{
  width:500rpx;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.middle-image{
  width: 250rpx;
  height: 250rpx;
  border-radius: 250rpx;
  overflow: hidden;
}
.order{
  width:100%;
  height:100%;
}
.middle-wz-big{
  font-size: 32rpx;
  color:  rgb(65, 65, 65);
  margin: 10rpx 0;
}
.middle-wz-small{
  font-size: 28rpx;
  color: rgb(139, 139, 139);
}